            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:text="Mevcut / Sökülen"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Baslik2" />

        <TextView
            android:id="@+id/BaslikTakilan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="Takılan"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@id/editGunduzTam"
            app:layout_constraintTop_toBottomOf="@+id/Baslik2" />

        <TextView
            android:id="@+id/textGunduz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="T1:"
            android:textSize="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/BaslikMevcutSokulen" />

        <EditText
            android:id="@+id/editGunduzTams"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:digits="0123456789"
            android:ems="5"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusDown="@+id/editGunduzKusurats"
            android:singleLine="true"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textGunduz"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textGunduz" />

        <TextView
            android:id="@+id/textGunduzNoktas"
            android:layout_width="5dp"
            android:layout_height="wrap_content"
            android:text=","
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/editGunduzTams"
            app:layout_constraintStart_toEndOf="@+id/editGunduzTams"
            app:layout_constraintTop_toTopOf="@+id/editGunduzTams" />

        <EditText
            android:id="@+id/editGunduzKusurats"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:digits="0123456789"
            android:ems="3"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusDown="@+id/editPuantTams"
            android:singleLine="true"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textGunduzNoktas"
            app:layout_constraintStart_toEndOf="@+id/textGunduzNoktas"
            app:layout_constraintTop_toTopOf="@+id/textGunduzNoktas" />

        <View
            android:id="@+id/cizik1"
            style="@style/Divider.Vertical"
            android:layout_width="1dp"
            android:layout_height="210dp"
            android:layout_marginTop="35dp"
            android:background="#A4C639"
            app:layout_constraintEnd_toStartOf="@+id/editGunduzTam"
            app:layout_constraintStart_toEndOf="@+id/editGunduzKusurats"
            app:layout_constraintTop_toTopOf="@+id/Baslik2" />

        <EditText
            android:id="@+id/editGunduzTam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:digits="0123456789"
            android:ems="5"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusDown="@+id/editGunduzKusurat"
            android:singleLine="true"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/editGunduzKusurats"
            app:layout_constraintEnd_toStartOf="@+id/textGunduzNokta"
            app:layout_constraintTop_toTopOf="@+id/editGunduzKusurats" />

        <TextView
            android:id="@+id/textGunduzNokta"
            android:layout_width="5dp"
            android:layout_height="wrap_content"
            android:text=","
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/editGunduzTam"
            app:layout_constraintEnd_toStartOf="@+id/editGunduzKusurat"
            app:layout_constraintTop_toTopOf="@+id/editGunduzTam" />

        <EditText
            android:id="@+id/editGunduzKusurat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:digits="0123456789"
            android:ems="3"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusDown="@+id/editPuantTam"
            android:singleLine="true"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/textGunduzNokta"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textGunduzNokta" />

        <TextView
            android:id="@+id/textPuant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="T2:"
            android:textSize="12dp"
            app:layout_constraintEnd_toEndOf="parent"
