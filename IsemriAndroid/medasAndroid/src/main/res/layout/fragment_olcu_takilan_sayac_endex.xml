<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="false"
    tools:context=".OlcuTakilanSayacEndexFragment">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".OlcuTakilanSayacEndexFragment">

        <TextView
            android:id="@+id/Baslik"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:text="Takılan Sayaç"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/takilanSerino_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:text="Seri No:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/takilanSerino"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:inputType="number"
            android:nextFocusDown="@+id/takilanMarkasi"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/takilanSerino_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/takilanSerino_text" />

        <TextView
            android:id="@+id/takilanMarkasi_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Markası:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/takilanSerino_text" />

        <Spinner
            android:id="@+id/takilanMarkasi"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:inputType="text"
            android:nextFocusDown="@+id/takilanAkimO"
            android:paddingTop="10dp"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingBottom="10dp"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/takilanMarkasi_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/takilanMarkasi_text" />

        <TextView
            android:id="@+id/takilanAkimO_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Akım Oranı:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/takilanMarkasi_text" />

        <EditText
            android:id="@+id/takilanAkimO"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:inputType="number"
            android:nextFocusDown="@+id/takilanGerilimO"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/takilanAkimO_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/takilanAkimO_text" />

        <TextView
            android:id="@+id/takilanGerilimO_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Gerilim Oranı:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/takilanAkimO_text" />

        <EditText
            android:id="@+id/takilanGerilimO"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:inputType="number"
            android:nextFocusDown="@+id/takilanSayacCarpani"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/takilanGerilimO_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/takilanGerilimO_text" />

        <TextView
            android:id="@+id/takilanSayacCarpani_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Sayaç Çarpanı:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/takilanGerilimO_text" />

        <EditText
            android:id="@+id/takilanSayacCarpani"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:inputType="numberDecimal"
            android:nextFocusDown="@+id/takilanHaneAdeti"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/takilanSayacCarpani_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/takilanSayacCarpani_text" />

        <TextView
            android:id="@+id/takilanHaneAdeti_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Hane Adeti:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/takilanSayacCarpani_text" />

        <EditText
            android:id="@+id/takilanHaneAdeti"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:inputType="number"
            android:nextFocusDown="@+id/takilanImalTarihi"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/takilanHaneAdeti_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/takilanHaneAdeti_text" />

        <TextView
            android:id="@+id/takilanImalTarihi_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="İmal Tarihi:"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/takilanHaneAdeti_text" />

        <EditText
            android:id="@+id/takilanImalTarihi"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:inputType="number"
            android:textSize="15sp"
            android:nextFocusDown="@+id/editMuhurNo"
            app:layout_constraintBottom_toBottomOf="@+id/takilanImalTarihi_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/takilanImalTarihi_text" />

<!-- yeni mühür -->

        <TextView
            android:id="@+id/BaslikYeniMuhur"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="24dp"
            android:text="Yeni Mühür"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/takilanImalTarihi_text" />

        <Spinner
            android:id="@+id/muhurSeri"
            android:layout_width="80sp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:ems="10"
            android:imeOptions="actionNext"
            android:maxLength="1"
            android:nextFocusForward="@+id/editMuhurNo"
            android:singleLine="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/BaslikYeniMuhur" />

        <EditText
            android:id="@+id/editMuhurNo"
            android:layout_width="235dp"
            android:layout_height="wrap_content"
            android:ems="8"
            android:hint="Mühür No Girin..."
            android:imeOptions="actionDone"
            android:inputType="number"
            android:maxLength="8"
            android:singleLine="true"
            android:textSize="15sp"
            android:nextFocusForward="@+id/editGunduzTams"
            app:layout_constraintBottom_toBottomOf="@+id/muhurSeri"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/muhurSeri" />

<!-- endeksler -->

        <TextView
            android:id="@+id/Baslik2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="24dp"
            android:text="Endeksler"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/muhurSeri" />

        <TextView
            android:id="@+id/BaslikMevcutSokulen"
